main:-
 io.read_token(io.stdin,I,S),
 loop(integer.of_str(S),I,io.stdout).
loop(0,I,O):-io.free_port(I),io.free_port(O).

loop($n,I0,O0):-$n>0|
 compute_sum(0,$n,0,I0,I1,O0,O),
 io.read_token(I1,I,S),
 loop(integer.of_str(S),I,O).

compute_sum($i,$n,$s,I0,I,O0,O):-
 $i=:=$n,int($s)|
 I0=I,io.print_line(O0,str.from($s),O).

compute_sum($i0,$n,$s,I0,I,O0,O):-
 $i0<$n,$i=$i0+1,int($s)|
io.read_token(I0,I1,S),
compute_sum($i,$n,$s+integer.of_str(S),I1,I,O0,O).