	cost($c,integer.set($a,$b),10000,null).
	makedata.cost($a,$b,$c):-
	int($a),int($b),int($c)|error(makedata_cost).

	makedata.cost($a,$b):-
	int($a),int($b)|
	cost(integer.set($a,$b),integer.set($a,$b),10000,null).
